<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="tpSacControl" />
        <binding name="tpSacControl_SSL">
          <security mode="TransportWithMessageCredential">
            <message clientCredentialType="Certificate" />
          </security>
        </binding>
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:8731/SACServiceControl/"
        binding="basicHttpBinding" bindingConfiguration="tpSacControl"
        contract="SACServiceControl.ISACServiceControl" name="tpSacControl" />
      <endpoint address="https://localhost:8737/SACServiceControl/"
        binding="basicHttpBinding" bindingConfiguration="tpSacControl_SSL"
        contract="SACServiceControl.ISACServiceControl" name="tpSacControl_SSL" />
    </client>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
</configuration>
